<?xml version="1.0" encoding="utf-8"?>

<manifest package="se.ekonomipuls" android:versionName="0.1.Pre-Alpha-5" android:versionCode="5" xmlns:android="http://schemas.android.com/apk/res/android">
	<application android:name="se.ekonomipuls.EkonomipulsApplication" android:debuggable="false" android:label="@string/app_homescreen" android:icon="@drawable/ic_launcher_ekonomipuls">
		<activity android:name="EkonomipulsHome" android:theme="@android:style/Theme.NoTitleBar" android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name=".reciever.BankDroidTransactionsReceiver">
			<intent-filter>
				<action android:name="com.liato.bankdroid.action.TRANSACTIONS"></action>
			</intent-filter>
		</receiver>
		<service android:enabled="true" android:name=".service.BankDroidImportService"></service>

		<activity android:name="OverviewSettings" android:theme="@android:style/Theme.NoTitleBar"></activity>


		<activity android:name="AddEditCategory" android:theme="@android:style/Theme.NoTitleBar"></activity>
		<activity android:name="FilterRuleOverview" android:theme="@android:style/Theme.NoTitleBar"></activity>
		<activity android:name="VerifyTransactions" android:theme="@android:style/Theme.NoTitleBar"></activity>
	</application>
	<uses-sdk android:minSdkVersion="7"/>
	<uses-permission android:name="com.liato.bankdroid.permission.READ_ACCESS_BANK_TRANSACTIONS"/>

</manifest> 
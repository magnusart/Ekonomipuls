<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="se.ekonomipuls" android:versionCode="1" android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:label="@string/app_name" android:name="EkonomipulsHome">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name=".reciever.BankDroidTransactionsReciever">
			<intent-filter>
				<action android:name="com.liato.bankdroid.action.TRANSACTIONS"></action>
			</intent-filter>
		</receiver>
		<service android:enabled="true"
			android:name=".service.imp.BankDroidImportService"></service>
	</application>
	<uses-sdk android:minSdkVersion="7" />
	<uses-permission
		android:name="com.liato.bankdroid.permission.READ_ACCESS_BANK_TRANSACTIONS" />
</manifest> 
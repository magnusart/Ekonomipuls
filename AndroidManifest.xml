<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="se.ekonomipuls" android:versionCode="1" android:versionName="1.0">
	<application android:label="@string/app_homescreen" android:debuggable="true" android:icon="@drawable/ic_launcher_ekonomipuls">
		<activity android:name="EkonomipulsHome" android:theme="@android:style/Theme.NoTitleBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name=".reciever.BankDroidTransactionsReciever">
			<intent-filter>
				<action android:name="com.liato.bankdroid.action.TRANSACTIONS"></action>
			</intent-filter>
		</receiver>
		<service android:enabled="true" android:name=".service.importer.BankDroidImportService"></service>
		<service android:enabled="true" android:name=".service.filter.TransactionsFilterService"></service>

	<activity android:name="OverviewSettings" android:theme="@android:style/Theme.NoTitleBar"></activity>


<activity android:name="AddEditCategory" android:theme="@android:style/Theme.NoTitleBar"></activity>
</application>
	<uses-sdk android:minSdkVersion="7" />
	<uses-permission
		android:name="com.liato.bankdroid.permission.READ_ACCESS_BANK_TRANSACTIONS" />

</manifest> 
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.magnusart.transtatistics" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:label="@string/app_name" android:name="TranStatistics">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<receiver android:name=".reciever.BankDroidTransactionReciever">
			<intent-filter>
				<action android:name="com.magnusart.transtatistics.action.UPDATE_TRANSACTIONS"></action>
			</intent-filter>
		</receiver>
	</application>
	<uses-sdk android:minSdkVersion="4" />

<uses-permission android:name="com.liato.bankdroid.premission.READ_ACCESS_BANK_TRANSACTIONS"></uses-permission>
</manifest> 